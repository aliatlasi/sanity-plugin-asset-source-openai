{
  "name": "sanity-plugin-asset-source-openai",
  "version": "1.1.7",
  "description": "generate images using Dall-e openai in your Sanity Studio",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "license": "MIT",
  "author": "Hrithik Prasad <hrithik@roboto.studio>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "run-s build",
    "watch": "pkg-utils watch --strict",
    "release": "standard-version && yarn run publish-pkg",
    "publish-pkg": "git push --follow-tags origin main"
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^1.0.14",
    "react-icons": "^4.7.1"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^2.2.1",
    "@sanity/plugin-kit": "^3.1.4",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "prettier-plugin-packagejson": "^2.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^4.1.1",
    "sanity": "^3.2.4",
    "standard-version": "^9.5.0",
    "styled-components": "^5.3.6",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=14"
  }
}
